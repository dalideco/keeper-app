,"29"],"C:\\Users\\dalis\\Desktop\\work\\web development\\react_projects\\full\\keeper\\src\\App.js",["30"],"import React,{useState,useEffect} from 'react';\nimport './App.scss';\nimport Navbar from  './components/Navbar';\nimport Main from './components/Main';\nimport axios from 'axios'\nimport qs from 'qs'\n\nfunction useForceUpdate(){\n  const [value,setValue] = useState(0);\n  return ()=> setValue(prev=>prev+1)\n}\n\nfunction App() {\n\n  const [database,setdatabase] = useState([]);\n  const forceUpdate = useForceUpdate();\n\n\n  useEffect( ()=>{\n     axios.get(\"https://keeper-app-dalideco.herokuapp.com/find\").then(response=>{\n      setdatabase(prev=>response.data);\n    }).catch(\n      err=> console.log(err)\n    )\n  })\n\n  const addToDatabase =(input)=>{\n    axios.post(\"https://keeper-app-dalideco.herokuapp.com/add\",qs.stringify(input)).then(response=>\n      forceUpdate()\n    ).catch(err=>console.log(err));\n    \n  }\n\n  const removingFromdatabase= (id)=>{\n    axios.post(\"https://keeper-app-dalideco.herokuapp.com/remove\",qs.stringify({_id: id})).then(response=>\n      forceUpdate()\n    ).catch(err=> console.log(err))\n    \n  }\n\n  return (\n    <div className=\"App\">\n      <Navbar />\n      <Main database={database} addToDatabase={addToDatabase}\n        removingFromdatabase={removingFromdatabase}/>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\dalis\\Desktop\\work\\web development\\react_projects\\full\\keeper\\src\\components\\Navbar.js",[],"C:\\Users\\dalis\\Desktop\\work\\web development\\react_projects\\full\\keeper\\src\\components\\Main.js",[],"C:\\Users\\dalis\\Desktop\\work\\web development\\react_projects\\full\\keeper\\src\\components\\Myform.js",[],"C:\\Users\\dalis\\Desktop\\work\\web development\\react_projects\\full\\keeper\\src\\components\\Block.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":9,"column":10,"nodeType":"37","messageId"